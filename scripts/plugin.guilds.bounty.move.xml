<script>
<name>plugin.guilds.bounty.move</name>
<version>2</version>
<engineversion>66</engineversion>
<description>Bounty Hunters Guild: Movement script for bounties</description>
<arguments>
<argument index="1" name="a.bounty" type="Value" desc="Bounty Data" />
</arguments>
<sourceplaintext>
<![CDATA[
  $DEBUG = [FALSE]
  
  * store the bounty data
  [THIS] ->set local variable: name='bounty' value=$a.bounty
  
  * start the watch script
  $tasks = [THIS] ->get all tasks with script on stack=plugin.guilds.bounty.watch
  if $tasks
    $i = size of array $tasks
    while $i
      dec $i = 
      $t = $tasks[$i]
      if [THIS] -> is script plugin.guilds.bounty.watch on stack of task=$t
        $task = $t
        break
      end
    end
  end
  skip if $task
    $task = [THIS] -> get next available task: starting=[Task.StartCustom]
  [THIS] ->start task $task with script plugin.guilds.bounty.watch and prio 100: arg1=$a.bounty arg2=null arg3=null arg4=null arg5=null
  
  * attach an event script for killing/capture
  $flags = [Event.Captured] | [Event.Killed] | [Event.GKilled]
  [THIS] ->attach event script: events=$flags, script=plugin.guilds.bounty.event
  
  * start sector, always stay within jumps of this sector
  $start.sector = $a.bounty['sector']
  do if $DEBUG
    write to player logbook: printf: type=[Log.Debug], fmt='%s: Starting bounty movement: %s (%s)', [THIS], [SECTOR], [ENVIRONMENT], null, null
  
  [THIS] ->set race logic control enabled to [FALSE]
  $speaker = [THIS] ->get pilot speaker
  if $speaker -> exists
    $speaker ->set local variable: name='bounty' value=$a.bounty
    $speaker ->set local variable: name='bounty.ship' value=[THIS]
  else
    do if $DEBUG
      write to player logbook: printf: type=[Log.Debug], fmt='####### Missing Pilot  Speaker: %s', [THIS], $speaker, [ENVIRONMENT], null, null
  end
  
  while [TRUE]
@   = wait randomly from 2000 to 5000 ms
    
  * fly station to station within sector
    $docks = random value from 1 to 4 - 1
    while $docks > 0
      $flags = [Find.DockingAllowed] | [Find.Random]
      $station = find station: sector=[SECTOR] class or type=null race=null flags=$flags refobj=[THIS] maxdist=null maxnum=null refpos=null
      do if $DEBUG
        write to player logbook: printf: type=[Log.Debug], fmt='%s: Searching station, tries=%s, Station=%s', [THIS], $docks, $station, null, null
      if $station -> exists
        dec $docks = 
@       = [THIS] -> call script !ship.cmd.movestation.std :  object to land on=$station
      else
        break
      end
      
  * if docked, wait around a small time
      if [THIS] -> is docked
@       = wait randomly from 5000 to 20000 ms
        do if $DEBUG
          write to player logbook: printf: type=[Log.Debug], fmt='%s: Undocking from station', [THIS], $docks, $station, null, null
@       = [THIS] -> call script !move.undock : 
      end
      
  * idle around in the sector for abit
      [THIS] ->set command: COMMAND_IDLE
      [THIS] ->set destination to null
      $idle = random value from 60000 to 180000 - 1
      do if $DEBUG
        write to player logbook: printf: type=[Log.Debug], fmt='%s: Idling for: %s', [THIS], $idle, $station, null, null
      while $idle > 0
        if [THIS] -> is in active sector
@         = [THIS] -> move around $idle ms
          $idle = 0
        else
@         = wait 10000 ms
          $idle = $idle - 10000
        end
      end
    end
    
  * move to a new sector
    $range = $a.bounty['range']
    skip if $range
      $range = random value from 3 to 5 - 1
    do if $DEBUG
      write to player logbook: printf: type=[Log.Debug], fmt='%s: searching for sector in range: %s', [THIS], $range, $station, null, null
    $sector = find a random sector: startsector=$start.sector, jumps=$range, owner=null
    if $sector -> exists
      do if $DEBUG
        write to player logbook: printf: type=[Log.Debug], fmt='%s: flying to new sector: %s', [THIS], $sector, $docks, null, null
@     = [THIS] -> call script !ship.cmd.movesector.std :  target sector=$sector
    end
  end
  
  do if $DEBUG
    write to player logbook: printf: type=[Log.Debug], fmt='%s: Break out of loop!!', [THIS], $sector, $docks, null, null
  return null
]]>
</sourceplaintext>
