<script>
<name>plugin.guilds.delayevent</name>
<version>1</version>
<engineversion>71</engineversion>
<description>Guilds: Send a delayed script event</description>
<arguments>
<argument index="1" name="a.event" type="Var/String" desc="Event" />
<argument index="2" name="a.obj" type="Value" desc="Object" />
<argument index="3" name="a.param" type="Value" desc="Param" />
<argument index="4" name="a.param2" type="Value" desc="Param2" />
<argument index="5" name="a.delay" type="Var/Number" desc="Delay" />
</arguments>
<sourceplaintext>
<![CDATA[
  while $a.delay > 0
@   $a.delay = wait $a.delay ms
  end
  send script event: name=$a.event, object=$a.obj, param1=$a.param, param2=$a.param2
  return null
]]>
</sourceplaintext>
