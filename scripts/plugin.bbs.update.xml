<script>
<name>plugin.bbs.update</name>
<version>1</version>
<engineversion>64</engineversion>
<description>Guilds, BBS - Update arcticles for sector</description>
<arguments>
<argument index="1" name="a.sector" type="Var/Sector" desc="Sector" />
<argument index="2" name="a.race" type="Var/Boolean" desc="Race" />
</arguments>
<sourceplaintext>
<![CDATA[
  $page.id = get global variable: name='pageid.guilds'
  $guilds.data = get global variable: name='guilds.data'
  $guilds.config = get global variable: name='guilds.config'
  if $guilds.data
    $sector.data = $guilds.data['bbs.sectors']
    if not $sector.data
      $sector.data = table alloc
      $guilds.data['bbs.sectors'] = $sector.data
    end
    
    $bbs.data = $sector.data[$a.sector]
    if not $bbs.data
      $bbs.data = table alloc
      $sector.data[$a.sector] = $bbs.data
    end
    
    $articles = $bbs.data['articles']
    if not $articles
      $articles = table alloc
      $bbs.data['articles'] = $articles
    end
    
    $all.articles = $guilds.data['bbs.articles']
    
  * check for any arcticles that have expired
    $current.time = playing time
    
    $key = get next key: table=$articles key=null
    while $key
      $a = $key
      $key = get next key: table=$articles key=$key
      
      $tArticle = $all.articles[$a]
      if $tArticle
        $expire = $tArticle['expire']
        if $current.time > $expire
          remove key $a from table $articles
        end
      end
    end
    
    $max = $bbs.data['max']
    $expire = $bbs.data['max.expire']
    if ! $max OR ( $expire AND $current.time > $expire )
      $min = $guilds.config['bbs.min']
      skip if $min
        $min = 3
      skip if $max
        $min = 5
      $max = $guilds.config['bbs.max']
      inc $max = 
      do if $min >= $max
        $min = $max - 1
      $max = random value from $min to $max - 1
      $expire = random value from 7200 to 14400 - 1
      $expire = $current.time + $expire
      $bbs.data['max'] = $max
      $bbs.data['max.expire'] = $expire
    end
    
  * check for existing dynamic articles
    $has.dynamic = 0
    $key = get next key: table=$articles key=null
    while $key
      $article = $all.articles[$key]
      if $article
        if $article['is.dynamic']
          inc $has.dynamic = 
        end
      end
      $key = get next key: table=$articles key=$key
    end
    
  * add at least 1 dynamic article, if it exists
    $current.time = playing time
    $value = $guilds.config['bbs.min.dynamic']
    while $has.dynamic < $value
      inc $has.dynamic = 
      $size = size of table $articles
      $max = $bbs.data['max']
      if $size < $max
@       $a = [THIS] -> call script plugin.bbs.getnews :  Sector=$a.sector  Race=$a.race  Include Dynamic=[TRUE]  Exclude=$articles  Only Dynamic=[TRUE]
        if $a
          gosub sub.AddArticle
        end
      end
    end
    
    $size = size of table $articles
    $max = $bbs.data['max']
    while $size < $max
@     $a = [THIS] -> call script plugin.bbs.getnews :  Sector=$a.sector  Race=$a.race  Include Dynamic=[TRUE]  Exclude=$articles  Only Dynamic=[FALSE]
      skip if $a
        break
      gosub sub.AddArticle
      inc $size = 
    end
    
    return $bbs.data
  end
  
  return null
  
  
  sub.AddArticle:
  $expire = random value from 2400 to 7200 - 1
  $expire = $expire + $current.time
  $articles[$a] = $expire
  gosub sub.CheckDynamic
  endsub
  
  
  sub.CheckDynamic:
  $article = $all.articles[$a]
  if $article
    if $article['is.dynamic']
      $max.articles = $article['max']
      if is datatype[ $max.articles ] == DATATYPE_INT
        dec $max.articles = 
        $article['max'] = $max.articles
      end
    end
  end
  endsub
  return null
]]>
</sourceplaintext>
