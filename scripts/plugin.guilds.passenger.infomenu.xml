<script>
<name>plugin.guilds.passenger.infomenu</name>
<version>1</version>
<engineversion>65</engineversion>
<description>Guilds: Passengers, info menu for passenger</description>
<arguments>
<argument index="1" name="a.obj" type="Value" desc="Object" />
<argument index="2" name="a.event" type="Value" desc="Event" />
<argument index="3" name="a.data" type="Value" desc="Data" />
</arguments>
<sourceplaintext>
<![CDATA[
  $page.id = get global variable: name='pageid.guilds'
  $passenger.data = get global variable: name='passengerinfo.data'
  $guilds.data = get global variable: name='guilds.data'
  
  if $a.event == [MEvent.Start]
    set global variable: name='passengerinfo.data' value=$a.data
    $passenger.data = $a.data
    gosub sub.BuildMenu
    return $input
    
  else if $a.event == [MEvent.Finished]
    set global variable: name='passengerinfo.data' value=null
  end
  
  return null
  
  
  sub.BuildMenu:
  $menu = create custom menu array
  
  $passenger.name = $passenger.data['name']
  $read.text = read text: page=$page.id id=100010
  $text = create text for custom menu, left=$read.text, right=$passenger.name
  add custom menu info line to array $menu: text=$text
  add info line section to menu: $menu
  
  $value = $passenger.data['sector']
  $read.text = read text: page=$page.id id=100011
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $passenger.data['cabin']
  $read.text = read text: page=$page.id id=100012
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $passenger.data['travel.type']
  $value = $value + 100020
  $value = read text: page=$page.id id=$value
  $read.text = read text: page=$page.id id=100013
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  add info line section to menu: $menu
  
  $value = $passenger.data['pay']
  $value = convert number $value to string
  $value = sprintf: fmt='%s  %s', $value, [Text.Credits], null, null, null
  $read.text = read text: page=$page.id id=100015
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $passenger.data['xp']
  $value = convert number $value to string
  $read.text = read text: page=$page.id id=100018
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $passenger.data['travel.type']
  if $value != 4
    $read.text = read text: page=$page.id id=100014
    add info line heading to menu: $menu, heading=$read.text
    
    $destination = $passenger.data['destination']
    if is datatype[ $destination ] == DATATYPE_ARRAY
      $sorted.array = $destination
      gosub sub.AddAllDestination
    else if is datatype[ $destination ] == DATATYPE_TABLE
      $sorted.array = sort table keys: table=$destination
      gosub sub.AddAllDestination
    else
      gosub sub.AddDestination
    end
  end
  
  set menu option: $menu, icon=22
  set menu option: $menu, no select lines=[TRUE]
  
  $passenger.name = $passenger.data['name']
  $read.text = read text: page=$page.id id=136
  $input = create input: id='passenger.info.menu' menu=$menu title=$read.text description=$passenger.name callback=null
  endsub
  return null
  
  
  sub.AddAllDestination:
  $i = 0
  $size = size of array $sorted.array
  while $i < $size
    $destination = $sorted.array[$i]
    inc $i = 
    gosub sub.AddDestination
  end
  endsub
  
  
  sub.AddDestination:
  if $destination -> is of class Sector
    $text.cols = array alloc: size=1
  *$text = create custom menu string icon: type=2, id=51, Y=0 Width=8 Height=8
  *$text = create custom menu string gui icon: 51, Y=0 Width=8 Height=8
  *$text = create custom menu column string, position=0 length=20 right=[FALSE] colour=null text=$text
  *$text.cols[0] = $text
    $text = $destination -> get name
    $text = create custom menu column string, position=50 length=0 right=[FALSE] colour=null text=$text
    $text.cols[0] = $text
  else
    $text.cols = array alloc: size=3
    $text = $destination ->create custom menu string object icon
    $text = create custom menu column string, position=0 length=10 right=[FALSE] colour=null text=$text
    $text.cols[0] = $text
    $text = $destination -> get name
    $text = create custom menu column string, position=10 length=200 right=[FALSE] colour=null text=$text
    $text.cols[1] = $text
    $text = $destination -> get sector
    $text = $text -> get name
    $text = create custom menu column string, position=-1 length=200 right=[TRUE] colour=null text=$text
    $text.cols[2] = $text
  end
  
  add custom menu info line to array $menu: text=$text.cols
  endsub
  return null
]]>
