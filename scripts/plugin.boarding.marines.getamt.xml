<name>plugin.boarding.marines.getamt</name>
<version>100</version>
<engineversion>65</engineversion>
<description>Improved Boarding: Marines - Get amount to add</description>
<arguments>
</arguments>
<sourceplaintext>
<![CDATA[
  $amt = 3
  
  $config = get global variable: name='guilds.config'
  if $config
    $amt = $config['marines.add']
  end
  
  * few
  if $amt == 0
    return 10
    
  * some
  else if $amt == 1
    return 25
    
  * half
  else if $amt == 2
    return 50
    
  * many
  else if $amt == 3
    return 75
    
  * all
  else if $amt == 2
    return 100
  end
  
  return 100
]]>
</sourceplaintext>
