<script>
<name>plugin.guilds.utils</name>
<version>3</version>
<engineversion>71</engineversion>
<description>Guilds: Util functions</description>
<arguments>
<argument index="1" name="a.type" type="Var/String" desc="type" />
<argument index="2" name="a.value" type="Value" desc="Value1" />
<argument index="3" name="a.value2" type="Value" desc="Value 2" />
</arguments>
<sourceplaintext>
<![CDATA[
  $config = get global variable: name='guilds.config'
  $data = get global variable: name='planets.data'
  
  * Gets XP needed
  if $a.type == 'getXP'
    if $a.value < 0
      return 0
    end
    $xp = ( $a.value * 1000 ) + ( ( $a.value + 1 ) * 100 )
    return $xp
    
  else if $a.type == 'getMilitaryData'
    $data = get global variable: name='guilds.data'
    if $data
      if $a.value -> exists
        if $a.value -> is of class Ship
          $owner = $a.value -> get true owner
        else
          $owner = $a.value -> get owner race
        end
      else if is datatype[ $a.value ] == DATATYPE_RACE
        $owner = $a.value
      end
      
      if $owner == Argon
        $military = 'military.argon'
      else if $owner == Terran
        $military = 'military.terran'
      end
      
      if $military
        $ret = $data[$military]
        return $ret
      end
    end
    
  else if $a.type == 'getXPPercent'
    if $a.value < 0
      return 0
    end
@   $xp.needed = [THIS] -> call script plugin.guilds.utils :  type='getXP'  Value1=$a.value  Value 2=null
    $ret = fixed multiply and divide: $a.value2 * 100 / $xp.needed
    return $ret
    
  else if $a.type == 'checkenv'
    if $a.value -> exists
      $env = $a.value -> get environment
      if not $env -> exists
        do if $a.value2
@         = wait $a.value2 ms
        $a.value ->destruct: show no explosion=[FALSE]
      else
        $speed = $a.value -> get current speed
        $x = $a.value -> get x position
        $y = $a.value -> get y position
        $z = $a.value -> get z position
        if $x == 0 AND $y == 0 AND $z == 0 AND $speed == 0
          do if $a.value2
@           = wait $a.value2 ms
          $a.value ->destruct: show no explosion=[FALSE]
        end
      end
      
    end
    
  else if $a.type == 'getTitle'
    $idx = find position of pattern '[title]' in $a.value
    if $idx != -1
      $idx = $idx + 7
      $endIdx = find position of pattern '[/title]' in $a.value
      if $endIdx != -1
        if $endIdx > $idx
          $len = $endIdx - $idx
          $title = get substring of $a.value offset=$idx length=$len
          return $title
        end
      end
    end
  else if $a.type == 'mainUniverse'
    $main.universe = get global variable: name='main.universe'
    skip if $main.universe -> exists
      $main.universe = get sector from universe index: x=0, y=0
    if not $main.universe -> exists
      $x = 0
      $y = 0
      $max.x = get max sectors in x direction
      $max.y = get max sectors in y direction
      while $x < $max.x
        inc $x = 
        while $y < $max.y
          inc $y = 
          $main.universe = get sector from universe index: x=$x, y=$y
          do if $main.universe -> exists
            break
        end
      end
    end
    return $main.universe
  end
  
  return null
]]>
