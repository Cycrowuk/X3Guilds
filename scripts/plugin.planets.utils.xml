<script>
<name>plugin.planets.utils</name>
<version>1</version>
<engineversion>63</engineversion>
<description>Planetary Economy: Util functions</description>
<arguments>
<argument index="1" name="a.type" type="Var/String" desc="type" />
<argument index="2" name="a.value" type="Value" desc="Value1" />
<argument index="3" name="a.value2" type="Value" desc="Value 2" />
</arguments>
<sourceplaintext>
<![CDATA[
  $config = get global variable: name='guilds.config'
  $data = get global variable: name='planets.data'
  
  * Gets the prospertiy rate
  if $a.type == 'getRate'
    if $config
      $max.prosperity = $config['max.prosperity']
      $max.prosperity.inc = $config['max.prosperity.inc']
    end
    skip if $max.prosperity
      $max.prosperity = 10
    skip if $max.prosperity.inc
      $max.prosperity.inc = 160
    
    $rate = linear interpolation: x1=0, x2=$max.prosperity, y1=40, y2=$max.prosperity.inc, value=$a.value
    return $rate
    
  * Get the production time, from prosperity rate
  else if $a.type == 'getTime'
    $ret = fixed multiply and divide: $a.value * 100 / $a.value2
    return $ret
    
  * get population adjusted production
  else if $a.type == 'getProdCount'
    if $data
      if is datatype[ $a.value ] == DATATYPE_SECTOR
        if $a.value -> exists
          $tSector = $data[$a.value]
          if $tSector
            if $tSector['pop']
              $pop = $a.value ->get sector population
              if $pop >= 1 AND $pop < 10
                $ret = linear interpolation: x1=1, x2=9, y1=80, y2=300, value=$pop
                do if is datatype[ $a.value2 ] == DATATYPE_STRING
                  $a.value2 = string $a.value2 to integer
                if is datatype[ $a.value2 ] == DATATYPE_INT
                  $ret = fixed multiply and divide: $a.value2 * $ret / 100
                  return $ret
                end
              end
            end
          end
        end
      end
    end
    return $a.value2
    
  * get propserity level inc
  else if $a.type == 'getLevelInc'
    return 20
    
  * increase the prosperity level
  else if $a.type == 'incProsperity'
    if is datatype[ $a.value ] == DATATYPE_SECTOR
      if $a.value -> exists
        $tSector = $data[$a.value]
        if $tSector
          $prosperity = $tSector['prosperity']
          $max.prosperity = $tSector['max.prosperity']
          $prosperity.level = $tSector['prosperity.level']
@         $value = [THIS] -> call script plugin.planets.utils :  type='getLevelInc'  Value1=$prosperity  Value 2=null
          $prosperity.level = $prosperity.level + $value
          
          if $prosperity.level >= 10000
            if $prosperity < $max.prosperity
              $prosperity.level = $prosperity.level - 10000
              inc $prosperity = 
              $tSector['prosperity'] = $prosperity
            else
              $prosperity.level = 10000
            end
          end
          $tSector['prosperity.level'] = $prosperity.level
        end
      end
    end
    
  * decrease the prosperity level
  else if $a.type == 'decProsperity'
    if is datatype[ $a.value ] == DATATYPE_SECTOR
      if $a.value -> exists
        $tSector = $data[$a.value]
        if $tSector
          $prosperity = $tSector['prosperity']
          $max.prosperity = $tSector['max.prosperity']
          $prosperity.level = $tSector['prosperity.level']
@         $value = [THIS] -> call script plugin.planets.utils :  type='getLevelInc'  Value1=$prosperity  Value 2=null
          
          $prosperity.level = $prosperity.level - $value
          if $prosperity.level < 0
            if $prosperity > 1
              dec $prosperity = 
              $prosperity.level = 10000 + $prosperity.level
              $tSector['prosperity'] = $prosperity
            else
              $prosperity.level = 0
            end
          end
          $tSector['prosperity.level'] = $prosperity.level
        end
      end
    end
  end
  
  return null
]]>
</sourceplaintext>
