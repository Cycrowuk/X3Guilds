<script>
<name>!lib.agent.races</name>
<version>3</version>
<engineversion>71</engineversion>
<description>Diplomacy library: Return an array of races to use in agent commands</description>
<arguments>
<argument index="1" name="a.influence" type="Var/Number" desc="Influence needed" />
<argument index="2" name="a.extraRaces" type="Value" desc="Extra Races" />
</arguments>
<sourceplaintext>
<![CDATA[
  if is diplomacy menu in tutorial mode
    $race = get diplomacy menu tutorial race
    $races = create new array, arguments=$race, null, null, null, null
    return $races
  end
  
  $races = array alloc: size=18
  $races[0] = Argon
  $races[1] = Boron
  $races[2] = Split
  $races[3] = Paranid
  $races[4] = Teladi
  $races[5] = Pirates
  $races[6] = Goner
  $races[7] = Terran
  $races[8] = Yaki
  $races[9] = OTAS
  $races[10] = TerraCorp
  $races[11] = Atreus
  $races[12] = NMMC
  $races[13] = Strong Arms
  $races[14] = Duke's
  $races[15] = Darkspace
  $races[16] = Industritech
  $races[17] = Beryll
  
  if $a.extraRaces
    if is datatype[ $a.extraRaces ] == DATATYPE_RACE
      append $a.extraRaces to array $races
    else if is datatype[ $a.extraRaces ] == DATATYPE_ARRAY
      $races = merge arrays, $races, $a.extraRaces, null, null, null
    else if is datatype[ $a.extraRaces ] == DATATYPE_TABLE
      $a.extraRaces = get keys: table=$a.extraRaces
      $races = merge arrays, $races, $a.extraRaces, null, null, null
    end
  end
  
  if $a.influence > 0
    $i = size of array $races
    while $i
      dec $i = 
      $r = $races[$i]
      $cost = get influence bonus decrease: race=$r amount=$a.influence
      $inf = get influence of Player -> $r
      do if $cost > $inf
        remove element from array $races at index $i
    end
  end
  
  return $races
]]>
</sourceplaintext>
