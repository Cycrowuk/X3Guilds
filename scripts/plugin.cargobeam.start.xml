<script>
<name>plugin.cargobeam.start</name>
<version>1</version>
<engineversion>61</engineversion>
<description>Cargo Beamer: activation script</description>
<arguments>
</arguments>
<sourceplaintext>
<![CDATA[
  $SAMPLE = 991
  $SAMPLE.LEN = 10
  
  $UPGRADE = get ware from subtype name 'SS_WARE_CY_CARGOBEAM'
  if not [THIS] -> get amount of ware $UPGRADE in cargo bay
    play sample 1134
    return null
  end
  
@ $energy.need = [THIS] -> call script plugin.cargobeam.getlaser :  Ship=[THIS]
  $this.laser = [THIS] -> get current laser strength
  if ( $energy.need * 3 ) >= $this.laser
    START speak text: page=[Page.MiscVoice] id=1265 priority=80
    return null
  end
  
  play sample $SAMPLE
  $sound = $SAMPLE.LEN
  
  $table.failed = table alloc
  
  [THIS] ->set local variable: name='cargobeam.running' value=[TRUE]
  while [THIS] -> get local variable: name='cargobeam.running'
    
    dec $sound = 
    if $sound <= 0
      $sound = $SAMPLE.LEN
      play sample $SAMPLE
    end
@   = wait 100 ms
    
    skip if [THIS] -> get amount of ware $UPGRADE in cargo bay
      break
    
    $aWares = find flying ware: sector=[SECTOR] maintype=null subtype=null flags=[Find.Multiple] refobj=[THIS] maxdist=10000 maxnum=100, refpos=null
    if $aWares
      
      $this.x = [THIS] -> get x position
      $this.y = [THIS] -> get y position
      $this.z = [THIS] -> get z position
      $this.size = [THIS] -> get size of object
      $this.dist = $this.size + 100
      
      $i = size of array $aWares
      while $i
        dec $i = 
        $w = $aWares[$i]
        
        do if $table.failed[$w]
          continue
        
        if $w ->is flying ware blueprint
        else
          $ware = $w -> get ware type code of object
          $ware.count = $w -> get flying ware count
          do if is fluff ware: ware=$ware
            continue
          skip if [THIS] -> can transport ware $ware
            continue
          $ship.free = [THIS] -> get free amount of ware $ware in cargo bay
          skip if $ship.free > $ware.count
            continue
        end
        
        $fw.x = $w -> get x position
        $fw.y = $w -> get y position
        $fw.z = $w -> get z position
        
  * compute the direction vector between this and the ware
        $dir.x = ( $this.x - $fw.x )
        $dir.y = ( $this.y - $fw.y )
        $dir.z = ( $this.z - $fw.z )
        
  * normalise the direction vector
        $length = ( $dir.x * $dir.x ) + ( $dir.y * $dir.y ) + ( $dir.z * $dir.z )
        $length = square root of $length
        
        $dir.x = fixed multiply and divide: $dir.x * 1000 / $length
        $dir.y = fixed multiply and divide: $dir.y * 1000 / $length
        $dir.z = fixed multiply and divide: $dir.z * 1000 / $length
        
        $x = $fw.x + ( $dir.x / 5 )
        $y = $fw.y + ( $dir.y / 5 )
        $z = $fw.z + ( $dir.z / 5 )
        
        $w ->force position: x=$x y=$y z=$z
        $dist = get distance between [THIS] and $w
        if $dist < $this.dist
          skip if [THIS] -> catch ware object $w
            $table.failed[$w] = [TRUE]
        end
      end
    end
    
    $this.laser = [THIS] -> get current laser strength
    $this.laser = $this.laser - $energy.need
    [THIS] ->set laser energy to $this.laser
    do if $this.laser <= 1
      break
  end
  
  play sample 1
  
  return null
]]>
</sourceplaintext>
