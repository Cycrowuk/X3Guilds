<script>
<name>lib.cycrow.menu.mystations</name>
<version>2</version>
<engineversion>62</engineversion>
<description>Cycrow Library: Creates a menu for my stations</description>
<arguments>
<argument index="1" name="a.ignore" type="Value" desc="Ignore Array" />
<argument index="2" name="a.race" type="Var/Race" desc="Race" />
<argument index="3" name="a.class" type="Value" desc="Class Filter" />
<argument index="4" name="a.title" type="Var/String" desc="Title" />
<argument index="5" name="a.returnMenu" type="Var/Boolean" desc="Return Menu?" />
<argument index="6" name="a.search" type="Var/String" desc="Search String" />
</arguments>
<sourceplaintext>
<![CDATA[
  skip if $a.class
    $a.class = Station
  
  $stations =  get station array: of race $a.race class/type=$a.class
  
  * first get all sectors
  $tSectors = table alloc
  $count = size of array $stations
  while $count
    dec $count = 
    $s = $stations[$count]
    if $a.ignore
      if is datatype[ $a.ignore ] == DATATYPE_ARRAY
        do if find $s in array: $a.ignore
          continue
      else if is datatype[ $a.ignore ] == DATATYPE_TABLE
        do if $a.ignore[$s]
          continue
      else if $a.ignore == $s
        continue
      end
    end
    
    if $a.search
      skip if case insensitive string match, is $a.search in $s
        continue
    end
    
    $sector = $s -> get sector
    $tStations = $tSectors[$sector]
    if not $tStations
      $tStations = table alloc
      $tSectors[$sector] = $tStations
    end
    $tStations[$s] = [TRUE]
  end
  
  if not size of table $tSectors
    if $a.returnMenu
      $menu = create custom menu array
      $read.text = read text: page=[Page.Menus] id=711
      add non selectable menu item: $menu, text=$read.text
      set menu option: $menu, icon=21
      return $menu
    end
    return null
  end
  
  * sort the sectors
  $aSorted = sort table keys: table=$tSectors
  
  $menu = create custom menu array
  
  * now display the stations, sector by sector
  $i = 0
  $count = size of array $aSorted
  while $i < $count
    $sector = $aSorted[$i]
    inc $i = 
    
    $text.cols = array alloc: size=2
    $text = $sector -> get name
    $text.col = create custom menu column string, position=0 length=250 right=[FALSE] colour=null text=$text
    $text.cols[0] = $text.col
    $read.text = read text: page=[Page.Menus] id=722
    $text.col = create custom menu column string, position=-1 length=100 right=[TRUE] colour=null text=$read.text
    $text.cols[1] = $text.col
    add custom menu heading to array $menu: title=$text.cols
    
    $tStations = $tSectors[$sector]
    $aStations = sort table keys: table=$tStations
    
    $s.count = size of array $aStations
    $s.i = 0
    while $s.i < $s.count
      $s = $aStations[$s.i]
      inc $s.i = 
      $text.cols = array alloc: size=3
      $text = $s ->create custom menu string object icon
      $text.col = create custom menu column string, position=0 length=10 right=[FALSE] colour=null text=$text
      $text.cols[0] = $text.col
      $text = $s -> get name
      $text.col = create custom menu column string, position=10 length=240 right=[FALSE] colour=null text=$text
      $text.cols[1] = $text.col
      $text = '*'
      $text.col = create custom menu column string, position=-1 length=100 right=[TRUE] colour=null text=$text
      $text.cols[2] = $text.col
      add custom menu item to array $menu: text=$text.cols returnvalue=$s
    end
  end
  
  set menu option: $menu, icon=21
  
  if $a.returnMenu
    return $menu
  end
  
  $text = read text: page=1265 id=27
  if $a.title
@   $ret = open custom menu: title=$text description=$a.title option array=$menu
  else
@   $ret = open custom menu: title=$text description=$text option array=$menu
  end
  skip if $ret == -1
    return $ret
  return null
]]>
</sourceplaintext>
