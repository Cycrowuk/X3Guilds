    set notoriety of Paranid -> ATF to [Notoriety.Enemy.MainSector] points
    set notoriety of Paranid -> Argon to [Notoriety.ArchEnemy] points
    set notoriety of Argon -> Paranid to [Notoriety.ArchEnemy] points
    set notoriety of Boron -> Split to [Notoriety.ArchEnemy] points
    set notoriety of Split -> Boron to [Notoriety.ArchEnemy] points
    
