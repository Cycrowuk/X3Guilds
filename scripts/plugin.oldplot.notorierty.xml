    set notoriety of ATF -> Paranid to [Notoriety.Foe] points
    set notoriety of Argon -> Terran to [Notoriety.Neutral] points
    set notoriety of Argon -> ATF to [Notoriety.Neutral] points
    set notoriety of Boron -> Terran to [Notoriety.Foe] points
    set notoriety of Boron -> ATF to [Notoriety.Foe] points
    set notoriety of Atreus -> Terran to [Notoriety.Foe] points
    set notoriety of Atreus -> ATF to [Notoriety.Foe] points
    set notoriety of Split -> Terran to [Notoriety.Foe] points
    set notoriety of Split -> ATF to [Notoriety.Foe] points
    set notoriety of Strong Arms -> Terran to [Notoriety.Foe] points
