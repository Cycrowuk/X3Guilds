<script>
<name>!move.jump</name>
<version>7</version>
<engineversion>72</engineversion>
<description>jump to a sector</description>
<arguments>
<argument index="1" name="target" type="Value" desc="gate or sector" />
<argument index="2" name="withfollowers" type="Var/Number" desc="followers should jump too" />
</arguments>
<sourceplaintext>
<![CDATA[
  if [THIS] -> is docked
@   = [THIS] -> call script !move.undock : 
  end
  
  $xenon.jump = [FALSE]
  if not [THIS] -> get true amount of ware Advanced Jumpdrive in cargo bay
    if not [THIS] -> get true amount of ware Jumpdrive in cargo bay
      if [OWNER] == Xenon
        $xenon.jump = [TRUE]
      else
        return FLRET_ERROR
      end
    end
  end
  
  $sector = $target
  skip if $target -> is of class Sector
    $sector = $target -> get sector
  
  if [THIS] -> get true amount of ware Advanced Jumpdrive in cargo bay
    $energy = 0
  else if [THIS] -> get true amount of ware Jumpdrive Energy Generator in cargo bay
    $energy = 0
  else
    $energy = [THIS] -> needed jump drive energy for jump to sector $sector
  end
  $nume = [THIS] -> get true amount of ware Energy Cells in cargo bay
  skip if $nume >= $energy
    return FLRET_ERROR
  
  if [THIS] -> is of class Carrier
@   = [THIS] -> call script !carrier.recall : 
  end
  
  * big ships take longer to jump than fighter escorts. So the escorts fly back
  * by having the big ship jump before tellings escorts to jump we avoid that
  if [THIS] -> is of class Big Ship
    if $xenon.jump
      $flags = [JumpFlg.NoDeviceCheck] | [JumpFlg.NoEnergyCheck] | [JumpFlg.NoJumpableCheck]
      $r = [THIS] ->jump: object=$target flags=$flags
    else if [THIS] -> get true amount of ware Advanced Jumpdrive in cargo bay
      $flags = [JumpFlg.NoDeviceCheck] | [JumpFlg.NoEnergyCheck] | [JumpFlg.NoJumpableCheck] | [JumpFlg.NoEnergyUse]
      $r = [THIS] ->jump: object=$target flags=$flags
    else if [THIS] -> get true amount of ware Jumpdrive Energy Generator in cargo bay
      $flags = [JumpFlg.NoDeviceCheck] | [JumpFlg.NoEnergyCheck] | [JumpFlg.NoJumpableCheck] | [JumpFlg.NoEnergyUse]
      $r = [THIS] ->jump: object=$target flags=$flags
    else
      $r = [THIS] -> use jump drive: target=$target
    end
    gosub Check.Jump.Capable
  end
  
  
  if $withfollowers
    $afollowers = [THIS] -> get formation follower ships
    if $afollowers
      $s = size of array $afollowers
      while $s > 0
        dec $s = 
        $ship = $afollowers[$s]
        $docked = $ship -> get environment
        if $docked != [THIS]
  * 12Jan09: only ships that are not docked at leader undock for jump
          $ship ->interrupt with script !ship.signal.leaderjumps and prio 20: arg1=[THIS] arg2=$target arg3=$withfollowers arg4=null
        end
      end
    end
  end
  
  * big ships should already have jumped
  if not [THIS] -> is of class Big Ship
    if $xenon.jump
      $flags = [JumpFlg.NoDeviceCheck] | [JumpFlg.NoEnergyCheck] | [JumpFlg.NoJumpableCheck] | [JumpFlg.NoEnergyUse]
      $r = [THIS] ->jump: object=$target flags=$flags
    else if [THIS] -> get true amount of ware Advanced Jumpdrive in cargo bay
      $flags = [JumpFlg.NoDeviceCheck] | [JumpFlg.NoEnergyCheck] | [JumpFlg.NoJumpableCheck] | [JumpFlg.NoEnergyUse]
      $r = [THIS] ->jump: object=$target flags=$flags
    else if [THIS] -> get true amount of ware Jumpdrive Energy Generator in cargo bay
      $flags = [JumpFlg.NoDeviceCheck] | [JumpFlg.NoEnergyCheck] | [JumpFlg.NoJumpableCheck] | [JumpFlg.NoEnergyUse]
      $r = [THIS] ->jump: object=$target flags=$flags
    else
      $r = [THIS] -> use jump drive: target=$target
    end
    gosub Check.Jump.Capable
  end
  return $r
  
  Check.Jump.Capable:
  skip if $r == FLRET_ERROR
    endsub
  [THIS] ->set command: COMMAND_MOVE_SECTOR
  if $target -> is of class Sector
    $sector = $target
  else
    $sector = $target -> get sector
  end
@ $r = [THIS] -> call script !move.movetosector.simple :  sector=$sector
  return $r
]]>
</sourceplaintext>
