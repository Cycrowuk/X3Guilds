<script>
<name>plugin.guilds.bounty.infomenu</name>
<version>1</version>
<engineversion>63</engineversion>
<description>Guilds: Bounty Hunters, info menu for bounty</description>
<arguments>
<argument index="1" name="a.obj" type="Value" desc="Object" />
<argument index="2" name="a.event" type="Value" desc="Event" />
<argument index="3" name="a.data" type="Value" desc="Data" />
</arguments>
<sourceplaintext>
<![CDATA[
  $page.id = get global variable: name='pageid.guilds'
  $bounty.data = get global variable: name='bountyinfo.data'
  $guilds.data = get global variable: name='guilds.data'
  
  if $a.event == [MEvent.Start]
    set global variable: name='bountyinfo.data' value=$a.data
    $bounty.data = $a.data
    gosub sub.BuildMenu
    return $input
    
  else if $a.event == [MEvent.Finished]
    set global variable: name='bountyinfo.data' value=null
  end
  
  return null
  
  
  sub.BuildMenu:
  $menu = create custom menu array
  
  $pilot = $bounty.data['pilot']
  $read.text = read text: page=[Page.Menus] id=838
  $text = create text for custom menu, left=$read.text, right=$pilot
  add custom menu info line to array $menu: text=$text
  $value = $bounty.data['ship']
  $read.text = read text: page=[Page.Menus] id=721
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $value -> get ID code
  $read.text = read text: page=$page.id id=1002
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  add info line section to menu: $menu
  
  $value = $bounty.data['sector']
  $read.text = read text: page=[Page.Menus] id=715
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $bounty.data['lastseen']
  if $value
    $read.text = read text: page=$page.id id=1020
    $text = create text for custom menu, left=$read.text, right=$value
    add custom menu info line to array $menu: text=$text
  end
  
  $read.text = read text: page=$page.id id=1021
  add info line heading to menu: $menu, heading=$read.text
  
  $value = $bounty.data['guild']
  if $value
    $value = $guilds.data[$value]['name']
    $value = read text: page=$page.id id=$value
    $read.text = read text: page=$page.id id=117
    $text = create text for custom menu, left=$read.text, right=$value
    add custom menu info line to array $menu: text=$text
  end
  
  $value = $bounty.data['type']
  $read.text = read text: page=$page.id id=1004
  if $value == 1
    $value = read text: page=$page.id id=1010
  else if $value == 2
    $value = read text: page=$page.id id=1011
  else if $value == 3
    $value = read text: page=$page.id id=1012
  else if $value == 4
    $value = read text: page=$page.id id=2000
  else
    $value = read text: page=$page.id id=1010
  end
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $bounty.data['pay']
  $value = convert number $value to string
  $read.text = read text: page=$page.id id=1005
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $bounty.data['xp']
  $value = convert number $value to string
  $read.text = read text: page=$page.id id=1016
  $text = create text for custom menu, left=$read.text, right=$value
  add custom menu info line to array $menu: text=$text
  
  $value = $bounty.data['story']
  if $value
    $read.text = read text: page=$page.id id=1137
    add info line heading to menu: $menu, heading=$read.text
    add custom menu info line to array $menu: text=$value
    $value = $bounty.data['story.seen']
    if $value
      add info line section to menu: $menu
      add custom menu info line to array $menu: text=$value
    end
    $value = $bounty.data['story.pay']
    if $value
      add info line section to menu: $menu
      add custom menu info line to array $menu: text=$value
    end
  end
  
  set menu option: $menu, icon=2
  set menu option: $menu, no select lines=[TRUE]
  
  $pilot = $bounty.data['pilot']
  $read.text = read text: page=$page.id id=110
  $input = create input: id='bounty.info.menu' menu=$menu title=$read.text description=$pilot callback=null
  endsub
  return null
]]>
</sourceplaintext>
