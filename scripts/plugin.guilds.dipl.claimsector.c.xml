<script>
<name>plugin.guilds.dipl.claimsector.c</name>
<version>1</version>
<engineversion>71</engineversion>
<description>Guilds: Diplomacy, claim sector, comman script</description>
<arguments>
<argument index="1" name="a.agent" type="Var/Agent" desc="Agent" />
<argument index="2" name="a.tasks" type="Var/Number" desc="Tasks" />
<argument index="3" name="a.tradeDock" type="Var/Station" desc="TradeDock" />
<argument index="4" name="a.sector" type="Var/Sector" desc="Sector" />
</arguments>
<sourceplaintext>
<![CDATA[
  if not $a.tradeDock -> exists
@   = [THIS] -> call script !agent.cmd.recall :  Diplomat=$a.agent  Tasks=null
    return null
  end
  
  $Guild.Data = get global variable: name='guilds.data'
  $page.id = get global variable: name='pageid.guilds'
  $cmd.id = $Guild.Data['diplomacy.cmd']['claim.sector']
  
@ $influence = [THIS] -> call script plugin.guilds.dipl.claimsector :  Agent=$a.agent  Event=[MEvent.Influence]  Data=null
  $influence = get influence bonus decrease: race=Privateer amount=$influence
  
  decrease influence of Player -> Privateer amount=$influence
  $a.agent ->inc agent statistic: id='egosoft.default.influence.used' amount=$influence
  
@ if not [THIS] -> call script !agent.move :  Diplomat=$a.agent  Target Station=$a.tradeDock
@   $a.agent = [THIS] -> call script !agent.cmd.recall :  Diplomat=$a.agent  Tasks=null
    return null
  end
  
  $a.agent ->inc agent statistic: id='egosoft.default.missions.diplomat' amount=1
  
  $read.text = sprintf: pageid=$page.id textid=92006, $a.sector, null, null, null, null
@ = [THIS] -> call script !lib.agent.initial.setup :  Diplomat=$a.agent  Menu Text=$read.text  Percent Delay=350
  
@ = [THIS] -> call script plugin.guilds.claimsector :  Sector=$a.sector  Faction=Player
  $read.text = sprintf: pageid=$page.id textid=92007, $a.sector, null, null, null, null
  display game tip: text=$read.text delay=5000
  
  return null
]]>
</sourceplaintext>
