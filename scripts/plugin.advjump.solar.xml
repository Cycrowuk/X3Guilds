<script>
<name>plugin.advjump.solar</name>
<version>301</version>
<engineversion>72</engineversion>
<description>Advanced Jumpdrive: Solar Generation</description>
<arguments>
</arguments>
<sourceplaintext>
<![CDATA[
  $ware = Solar Energy Generator
  
  * default station to use for production numbers
  $spp.type = Argon Solar Power Plant M
  
  $storeData = [THIS] -> get local variable: name='advjump.ec.data'
  if not $storeData
    $storeData = table alloc
    [THIS] ->set local variable: name='advjump.ec.data' value=$storeData
  end
  
  * convert old local variable
  $processing.left = [THIS] -> get local variable: name='advjump.ec.proc'
  if $processing.left
    $storeData['proc'] = $processing.left
    [THIS] ->set local variable: name='advjump.ec.proc' value=null
  end
  
  * in the mididle of processing, restart it
  $processing.left = $storeData['proc']
  if $processing.left > 0
    gosub sub.GetProduction
    $processing.time.left = $single.cycle.time + $current.time
@   = wait 1000 ms
  end
  
  
  while [THIS] -> get amount of ware $ware in cargo bay
  * restart task if new version is available
    if is a new script version available
      $task = get task ID
      $script = get script name
@     START [THIS] -> call script !lib.restart.task :  Object=[THIS]  Script=$script  Task=$task  Argument 1=null  Argument 2=null  Argument 3=null  Argument 4=null  Argument 5=null
      return null
    end
    
@   = wait 2000 ms
    
    gosub sub.GetProduction
    
  * currently processing crystal
    if $processing.left > 0
      $current.time = playing time
      if [THIS] -> get free amount of ware Energy Cells in cargo bay
        
  * check the resupply settings
        if [THIS] -> get jumpdrive fuel resupply
          $needed = [THIS] -> get resupply fuel required
          if $needed <= 0
@           = wait 10000 ms
            continue
          end
        end
        
        if $current.time > $processing.time.left
          dec $processing.left = 
          $processing.time.left = $single.cycle.time + $current.time
          $storeData['start'] = $current.time
          $storeData['proc'] = $processing.left
          = [THIS] -> add 1 units of Energy Cells
        end
      end
    else
  * start working on next crystal
      if [THIS] -> get amount of ware Crystals in cargo bay
        $processing.left = $production.count
        $current.time = playing time
        $processing.time.left = $single.cycle.time + $current.time
        $storeData['start'] = $current.time
        $storeData['proc'] = $processing.left
        = [THIS] -> add -1 units of Crystals
      else
@       = wait 10000 ms
      end
    end
    
  end
  
  return null
  
  
  sub.GetProduction:
  * get the solar efficeny of the sector
  $sector.energy = [SECTOR] ->get sun energy factor
  $eff = fixed multiply: $sector.energy * 8
  * computer the production cycle
  $production.time = get production time: stationtype=$spp.type, efficiency=$eff, player owned=[TRUE], scale factor=0
  $production.count = get production amount: stationtype=$spp.type, efficiency=$eff, player owned=[TRUE], scale factor=0
  $production.consumed = get consumed ware table: stationtype=$spp.type, efficiency=$eff, player=[TRUE], scale factor=0
  * Adjust production for a single cystal
  * Reduce efficent, taking 10 times as long and producing half as much
  $production.crystals = $production.consumed[Crystals]
  $production.count = ( $production.count / $production.crystals ) / 6
  $production.time = $production.time * 2
  $single.cycle.time = $production.time / $production.count
  $storeData['count'] = $production.count
  $storeData['time'] = $single.cycle.time
  endsub
  
  
  return null
]]>
</sourceplaintext>
