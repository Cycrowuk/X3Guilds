<script>
<name>plugin.scs.cmd.menu</name>
<version>100</version>
<engineversion>65</engineversion>
<description>Salvage Claim Software: Preload script for command</description>
<arguments>
<argument index="1" name="a.obj" type="Value" desc="Object" />
<argument index="2" name="a.event" type="Var/String" desc="Event" />
<argument index="3" name="a.value" type="Value" desc="Values" />
</arguments>
<sourceplaintext>
<![CDATA[
  $pageid = get global variable: name='pageid.guilds'
  
  if $a.event == [MEvent.Start]
    gosub sub.BuildMenu
    return $input
    
  else if $a.event == [MEvent.Returned]
    $menu.id = $a.value[0]
    $selection = $a.value[1]
    $value = $a.value[2]
    if $menu.id == 'scs.menu'
      if $selection == 'select.ship'
        if $value -> exists
          $input = create menu return to previous menu: value=$value
          return $input
        end
      end
    end
    
  else if $a.event == 'scs.menu'
    do if is datatype[ $a.value ] == DATATYPE_ARRAY
      $a.value = $a.value[0]
    if $a.value -> exists
      set global variable: name='scs.target' value=$a.value
      return [TRUE]
    end
    
  else if $a.event == 'select.ship'
    if $a.value -> exists
      set global variable: name='scs.target' value=$a.value
      return [TRUE]
    end
    
  else if $a.event == 'ship.select'
    gosub sub.SelectShip
    return $input
    
  else if $a.event == [MEvent.Arguments]
    $target = get global variable: name='scs.target'
    $ret = create new array, arguments=$target, null, null, null, null
    return $ret
    
  else if $a.event == [MEvent.Finished]
    set global variable: name='scs.target' value=null
  end
  
  return null
  
  
  sub.BuildMenu:
  $table.items = table alloc
  $obj.sector = $a.obj -> get sector
  $flags = [Find.Multiple] | [Find.Visible]
  $ships = find ship: sector=$obj.sector class or type=Ship race=Neutral Race flags=$flags refobj=$a.obj maxdist=null maxnum=100 refpos=null
  if $ships
    $i = size of array $ships
    $table.ships = table alloc
    while $i
      dec $i = 
      $s = $ships[$i]
      skip if $s ->can player see
        continue
@     if $null -> call script plugin.scs.valid :  Target=$s
        $dist = get distance between $a.obj and $s
        $table.ships[$s] = $dist
      end
    end
    
    if size of table $table.ships
      $sorted.array = sort table keys by value: table=$table.ships
      $target = get first item in array: $sorted.array
      $text = sprintf: pageid=$pageid textid=3083, $target, null, null, null, null
      $table.items[$target] = $text
    end
  end
  
  $target = get player tracking aim
@ if $null -> call script plugin.scs.valid :  Target=$target
    $text = sprintf: pageid=$pageid textid=3082, $target, null, null, null, null
    $table.items[$target] = $text
  end
  
  if size of table $table.items
    $menu = create custom menu array
    
    $text = get command info: command=COMMAND_CYCROW_CLAIM
    add custom menu info line to array $menu: text=$text
    
    $key = get next key: table=$table.items key=null
    while $key
      $text = $table.items[$key]
      add custom menu item to array $menu: text=$text returnvalue=$key
      $key = get next key: table=$table.items key=$key
    end
    
    add section to custom menu: $menu
    $text = read text: page=$pageid id=3084
    add custom menu item to array $menu: text=$text nextinput='ship.select' value=null
    
    set menu option: $menu, icon=12
    
    $title = read text: page=$pageid id=3081
    $text = read text: page=$pageid id=3085
    $input = create input: id='scs.menu' menu=$menu title=$title description=$text callback=null
  else
    gosub sub.SelectShip
  end
  endsub
  
  
  sub.SelectShip:
  $text = read text: page=$pageid id=3085
  $input = create input: id='select.ship', user input=Var/Ship, refobj=$a.obj, description=$text, without sector=[FALSE], filterclass=null, racemask=Neutral Race, flags=null, default=null
  endsub
  
  
  return null
]]>
</sourceplaintext>
