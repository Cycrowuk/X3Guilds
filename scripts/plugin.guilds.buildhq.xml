<script>
<name>plugin.guilds.buildhq</name>
<version>2</version>
<engineversion>72</engineversion>
<description>Guilds: Build a HQ</description>
<arguments>
<argument index="1" name="a.guild" type="Value" desc="Guild" />
<argument index="2" name="a.data" type="Value" desc="Data" />
<argument index="3" name="a.isHQ" type="Var/Boolean" desc="Is HQ?" />
</arguments>
<sourceplaintext>
<![CDATA[
  skip if $a.data
    return null
  
  $page.id = get global variable: name='pageid.guilds'
  $guilds.data = get global variable: name='guilds.data'
  if $a.guild
    $tGuild = $guilds.data[$a.guild]
  end
  
  $sector = $a.data['sector']
  skip if $sector -> exists
    return null
  
  do if $a.data['nobuild']
    return null
  
  $type = $a.data['type']
  
  $pos = $a.data['pos']
  if is datatype[ $pos ] == DATATYPE_ARRAY
    $x = $pos[0]
    $y = $pos[1]
    $z = $pos[2]
  else
    $x = random value from -30000 to 30000 - 1
    $y = random value from -5000 to 5000 - 1
    $z = random value from -30000 to 30000 - 1
  end
  
  $class = get ship class from subtype: $type
  
  $race = $a.data['race']
  if not $race
    $race = Guilds
  else if not is datatype[ $race ] == DATATYPE_RACE
    $race = Guilds
  end
  
  if $class -> is of class Station
    $hq = create station: type=$type owner=$race addto=$sector x=$x y=$y z=$z
    = $hq -> install 15 units of 2 GJ Shield
    $max.shield = $hq -> get maximum shield strength
    $hq ->set current shield strength to $max.shield
    if $tGuild
      if $tGuild['member']
        $hq ->set known status to [TRUE]
      end
    end
  else if $class -> is of class Ship
    $hq = create ship: type=$type owner=$race addto=$sector x=$x y=$y z=$z
    $hq ->disable ship rebuild
    $hq ->set pirate cover state to [FALSE]
    $hq ->set race logic control enabled to [FALSE]
    $hq ->add default items to ship
    if $tGuild
      if $tGuild['member']
        $hq ->set allow player docking: [TRUE]
      end
    end
  end
  
  skip if $hq -> exists
    return null
  
  if not $a.data['fixed.pos']
    $hq ->set safe position on activate: [TRUE]
  end
  
  do if $a.isHQ
    $hq ->set local variable: name='guild.hq' value=[TRUE]
  $hq ->set local variable: name='guild.owner' value=$a.guild
  $hq ->set local variable: name='guild.data' value=$a.data
  
  $name = $a.data['name']
  if $name
    $name = read text: page=$page.id id=$name
    $hq ->set name to $name
  end
  
  $name.id = $a.data['nameid']
  do if $name.id
    $hq ->set override name id: $name.id
  
  if $tGuild
    if $a.isHQ
      $event = sprintf: fmt='guild.hq.%s', $a.guild, null, null, null, null
      send script event: name=$event, object=$hq, param1=$a.guild, param2=null
    end
    if $hq -> is of class Station
      $array.sales = $a.data['sales']
      if $array.sales
        $level = $tGuild['level']
        skip if $level
          $level = 0
@       = $hq -> call script plugin.guilds.adjusthq :  Guild Level=$level  Sales Data=$array.sales
      end
    end
  end
  
  $a.data['destroyed'] = 0
  $a.data['obj'] = $hq
  
  $events = [Event.Destruct]
  $hq ->attach event script: events=$events, script=plugin.guilds.hq.event
  
  * Hack to fill the external docking ports of the shadow blade
  if $hq -> is of class Ship
    $speaker = $hq ->get pilot speaker
    if not $speaker
      $race = $a.data['subrace']
      skip if $race
        $race = $a.data['race']
@     = $hq -> call script lib.cycrow.createspeaker :  Race=$race
      $race = $a.data['race']
      $hq ->set owner race to $race
    end
    if $hq -> is of type Shadowblade
      $ship = create ship: type=Pirate Ship owner=$race addto=$sector x=$x y=$y z=$z
      $ship ->set pirate cover state to [FALSE]
@     START $ship -> call script !move.movetostation :  station=$hq
      $ship = create ship: type=Pirate Ship owner=$race addto=$sector x=$x y=$y z=$z
      $ship ->set pirate cover state to [FALSE]
@     START $ship -> call script !move.movetostation :  station=$hq
    end
  end
  
  return $hq
]]>
</sourceplaintext>
