<script>
<name>plugin.guilds.localescorts</name>
<version>1</version>
<engineversion>71</engineversion>
<description>Guilds: Find local escorts to add to guild board</description>
<arguments>
<argument index="1" name="a.guild" type="Var/String" desc="Guild" />
</arguments>
<sourceplaintext>
<![CDATA[
  $Guilds.Data = get global variable: name='guilds.data'
  skip if $Guilds.Data
    return null
  $tGuild = $Guilds.Data[$a.guild]
  skip if $tGuild
    return null
  skip if $tGuild['member']
    return null
  
  $guild.level = $tGuild['level']
  
  $table.ships = table alloc
  
  $table = [SECTOR] ->get ship table from sector/ship/station
  gosub sub.DoTable
  
  $station.table = [SECTOR] ->get station table from sector
  if $station.table
    $station = get next key: table=$station.table key=null
    while $station
      $table = $station ->get ship table from sector/ship/station
      gosub sub.DoTable
      $station = get next key: table=$station.table key=$station
    end
  end
  
  $ret = table alloc
  
  $ship = get next key: table=$table.ships key=null
  while $ship
    if not $ship -> get local variable: name='merc.escort'
      $environment = $ship -> get environment
      $ship.sector = $ship -> get sector
      $destination = $table.ships[$ship]
      $dest.sector = $destination -> get sector
      $jumps = get jumps from sector $dest.sector to sector $ship.sector
      
      $level = random value from 0 to $guild.level - 1
      inc $level = 
      $base.cost = random value from 150000 to 200000 - 1
      $price = ( $base.cost * $level * $jumps ) + ( 20000 * $level * $level )
      $xp = random value from 35 to 60 - 1
      $xp = $xp * $level * $jumps
      $data = create new array, size=6, elements=$ship, $destination, $jumps, $level, $price, $xp, null, null
      $ret[$ship] = $data
    end
    $ship = get next key: table=$table.ships key=$ship
  end
  
  
  return $ret
  
  
  sub.DoTable:
  if $table
    $ship = get next key: table=$table key=null
    while $ship
      gosub sub.CheckShip
      $ship = get next key: table=$table key=$ship
    end
  end
  endsub
  
  
  sub.CheckShip:
  if $ship -> is script !job.trade.freetrader on stack of task=0
    gosub sub.AddShip
  else if $ship -> is script !job.trade.freetrader.docksonly on stack of task=0
    gosub sub.AddShip
  else if $ship -> is script !job.trade.freetrader.raceonly on stack of task=0
    gosub sub.AddShip
  else if $ship -> is script !job.trade.docktrader on stack of task=0
    gosub sub.AddShip
  else if $ship -> is script !job.trade.blackmarket on stack of task=0
    gosub sub.AddShip
  else if $ship -> is script !job.trade.supplytrader on stack of task=0
    gosub sub.AddShip
  end
  endsub
  
  
  sub.AddShip:
  if not $ship -> get amount of ware Advanced Jumpdrive in cargo bay
    if not $ship -> get amount of ware Jumpdrive in cargo bay
      $destination = $ship -> get destination
      $trade.route = $ship -> get local variable: name='trade.route'
      if $destination
        $dest.sector = $destination -> get sector
        $ship.sector = $ship -> get sector
        $jumps = get jumps from sector $dest.sector to sector $ship.sector
        if $jumps > 2
          $table.ships[$ship] = $destination
        else if $jumps == 0
          $trade.route = $ship -> get local variable: name='trade.route'
          if $trade.route
            $source = $trade.route[1]
            $source.sector = $source -> get sector
            if $source.sector == [SECTOR]
              $destination = $trade.route[2]
              $dest.sector = $destination -> get sector
              $jumps = get jumps from sector $dest.sector to sector $ship.sector
              do if $jumps > 2
                $table.ships[$ship] = $destination
            end
          end
        end
      end
    end
  end
  endsub
  return null
]]>
</sourceplaintext>
