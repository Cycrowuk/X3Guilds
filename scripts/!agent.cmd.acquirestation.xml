<script>
<name>!agent.cmd.acquirestation</name>
<version>4</version>
<engineversion>72</engineversion>
<description>Diplomat Command: Assign diplomat to aquire station blueprints</description>
<arguments>
<argument index="1" name="a.diplomat" type="Var/Agent" desc="Diplomat" />
<argument index="2" name="a.tasks" type="Var/Number" desc="Tasks" />
<argument index="3" name="a.station" type="Var/Station" desc="Target station" />
<argument index="4" name="a.blueprint" type="Var/Ware" desc="Blueprint" />
<argument index="5" name="a.influence" type="Var/Number" desc="Influence" />
</arguments>
<sourceplaintext>
<![CDATA[
  * no target? recall the diplomat back to thier homebase
  if not $a.station -> exists
    return null
  end
  
  $noto = get notority require to buy ware: $a.blueprint
  $noto.level = get notoriety level: amount=$noto
  
  * comput any random factors at the begining
  $price = get average price of ware $a.blueprint
  $min.price = ( $price * 11 ) / 4
  $max.price = ( $price * 13 ) / 4
  $price = random value from $min.price to $max.price - 1
  
  $time = ( ( $noto.level + 5 ) * 100 ) + 600
  $min.time = ( $time * 3 ) / 4
  $max.time = ( $time * 5 ) / 4
  $time = random value from $min.time to $max.time - 1
  
  $race = $a.station -> get owner race
  $failed = [FALSE]
@ $chance = [THIS] -> call script !lib.agent.acquirechance :  Ware=$a.blueprint  Value Step=200000  Tasks=$a.tasks  Target=$a.station
  $rnd = random value from 0 to 100 - 1
  skip if $rnd < $chance
    $failed = [TRUE]
  
  decrease influence of Player -> $race amount=$a.influence
  $a.diplomat ->inc agent statistic: id='egosoft.default.influence.used' amount=$a.influence
  $a.diplomat ->inc agent statistic: id='egosoft.default.missions.diplomat' amount=1
  
  * move the diplomat to the target station
@ if not [THIS] -> call script !agent.move :  Diplomat=$a.diplomat  Target Station=$a.station
@   = [THIS] -> call script !agent.cmd.recall :  Diplomat=$a.diplomat  Tasks=0
    return null
  end
  
@ = [THIS] -> call script !lib.agent.initial.setup :  Diplomat=$a.diplomat  Menu Text=1104256  Percent Delay=2000
  
  * start to acquire discount
  $a.diplomat ->set agent start timer: [FALSE]
  $start.time = playing time
  $current.time = playing time
  $end.time = $start.time + $time
  
  $text = sprintf: pageid=35 textid=1104274, $a.blueprint, null, null, null, null
  $a.diplomat ->set agent task status: $text
  
  while $current.time < $end.time
    $current.time = playing time
@   = wait 500 ms
    $progress = ( ( $current.time - $start.time ) * 100 ) / $time
    $a.diplomat ->set agent task progress: $progress
    $text = sprintf: pageid=35 textid=1104274, $a.blueprint, null, null, null, null
    $a.diplomat ->set agent task status: $text
  end
  
@ = wait 1000 ms
  
  $last.chance = $a.station -> get local variable: name='diplomacy.last.chance'
  skip if $last.chance
    $last.chance = table alloc
  $chance = $last.chance[$a.blueprint]
  skip if is datatype[ $chance ] == DATATYPE_INT
    $chance = 0
  $chance = $chance + 15
  $last.chance[$a.blueprint] = $chance
  $a.station ->set local variable: name='diplomacy.last.chance' value=$last.chance
  
  if $failed
    $text = sprintf: pageid=35 textid=1104275, $a.blueprint, null, null, null, null
    display game tip: text=$text delay=0
    $text = sprintf: pageid=35 textid=1104276, $a.diplomat, $a.blueprint, null, null, null
    send incoming message $text to player: type:[Log.Event], display it=[FALSE], log it=[TRUE]
  *write to player logbook: printf: type=[Log.Event], fmt=$text, null, null, null, null, null
  else
    increase stat counter: textid=5370, amount=1
    $price.text = convert number $price to string
    $msg = sprintf: pageid=35 textid=1104277, $a.blueprint, $price.text, null, null, null
    $data.array = create new array, arguments=$a.blueprint, $price, null, null, null
    $text = read text: page=[Page.Menus] id=1102701
    $text = sprintf: fmt='%s: %s', $text, $a.blueprint, null, null, null
    $a.diplomat ->add command completion: title=$text, message=$msg, cost=$price, data=$data.array, script=!agent.acquire.blueprint
  end
  
  return null
]]>
</sourceplaintext>
