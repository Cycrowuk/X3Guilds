<script>
<name>!lib.diplomacy.gettradedock</name>
<version>4</version>
<engineversion>71</engineversion>
<description>Diplomacy Library: get a trade dock selection</description>
<arguments>
<argument index="1" name="a.influence" type="Var/Number" desc="Influence Needed" />
<argument index="2" name="a.text" type="Var/String" desc="Description" />
<argument index="3" name="a.input" type="Var/String" desc="Input ID" />
<argument index="4" name="a.extraRaces" type="Value" desc="ExtraRaces" />
</arguments>
<sourceplaintext>
<![CDATA[
  $classes = create new array, arguments=Trading Dock, Pirate Dock, Goner Temple, Goner Temple, Terracorp HQ
  append Atreus Headquarters to array $classes
  append OTAS HQ to array $classes
  append Duke's Haven to array $classes
  append NMMC Headquarters to array $classes
  append Strong Arms HQ to array $classes
  append Pirate Anarchy Port to array $classes
  append Darkspace Installation to array $classes
  append Industritech Enterprises to array $classes
  append Beryll Research Station to array $classes
  append Privateer Support Station to array $classes
  append Privateer Supply Base to array $classes
  
@ $aRaces = [THIS] -> call script !lib.agent.races :  Influence needed=$a.influence  Extra Races=$a.extraRaces
  skip if $a.input
    $a.input = 'station'
  
  * return a station user input
  $hq = get player HQ
  skip if $hq -> exists
    $hq = [PLAYERSHIP]
  
  $input = create input: id=$a.input, user input=Var/Station, refobj=$hq, description=$a.text, without sector=[FALSE], filterclass=$classes, racemask=$aRaces, flags=null, default=null
  return $input
  
  return null
]]>
</sourceplaintext>
